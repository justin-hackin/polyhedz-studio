{
  "name": "polyhedz-studio",
  "version": "0.0.1-alpha.4",
  "description": "A desktop application for the fabrication of flat-pack interlocking pyramids that form 3D stars and domes (geometric polyhedra).",
  "author": "Justin Barca <justinbarca@gmail.com>",
  "license": "MIT",
  "homepage": "http://bit.ly/polyhedz-deco",
  "private": true,
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "scripts": {
    "lint": "eslint . --ext js,ts,tsx",
    "lint-fix": "eslint . --ext js,ts,tsx --fix",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "watch": "vite",
    "build": "vite build",
    "serve-build": "http-server dist/ -p 3000"
  },
  "browserslist": [
    "Chrome 93"
