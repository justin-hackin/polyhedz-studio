{
  "name": "polyhedral-net-factory",
  "version": "0.0.1-alpha.1",
  "description": "A desktop application for the fabrication of flat-pack interlocking pyramids that form 3D stars and domes (geometric polyhedra)",
  "homepage": "http://bit.ly/polyhedz-deco",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "lint": "eslint src/ --ext .ts,.tsx --fix",
    "start": "node node_modules/electron/cli.js .",
    "release": "npm run compile && electron-builder",
    "web-build": "npm run web-clean-dist && webpack --config=src/web/webpack.config.js --mode production",
    "web-clean-dist": "rimraf _static/*",
    "web-start": "npm run web-start-dev",
    "web-start-dev": "webpack-dev-server --config=src/web/webpack.config.js --mode development",
    "web-start-prod": "npm run web-build && http-server _static/ -p 3000"
  },
