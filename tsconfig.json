{
  "compilerOptions": {
    "lib": [
      "es2015",
      "dom"
    ],
    "jsx": "react",
    "allowUmdGlobalAccess": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "paths": {
      "~*": ["./app/*"]
    },
    "target": "es2015"
  }
}
